# Environment
NODE_ENV=development
PORT=3000

# Database
MONGODB_URI=mongodb://localhost:27017/turnos-pro

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Setup (para crear primer admin - one-time use)
# Cambia a false despu√©s de crear el primer admin para deshabilitar permanentemente
SETUP_ENABLED=true
SETUP_TOKEN=changeme-strong-token-for-admin-setup

# CORS
CORS_ORIGIN=http://localhost:4200
# Kiosco URL (opcional, para CORS del kiosco Astro)
KIOSK_URL=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
